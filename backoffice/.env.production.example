# Production Environment Variables
# Copy this file to .env.production and update values

# API Configuration
REACT_APP_API_URL=http://yuklegeltaksiapi.istekbilisim.com
REACT_APP_BACKEND_URL=http://yuklegeltaksiapi.istekbilisim.com
REACT_APP_SOCKET_URL=ws://yuklegeltaksiapi.istekbilisim.com

# Application Settings
REACT_APP_ENVIRONMENT=production
REACT_APP_APP_NAME=YukleGel Taksi Backoffice
REACT_APP_COMPANY_NAME=YukleGel Taksi

# Feature Flags
REACT_APP_ENABLE_REAL_TIME_NOTIFICATIONS=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG_MODE=false

# Security
REACT_APP_SESSION_TIMEOUT=3600
REACT_APP_ENCRYPTION_KEY=your-encryption-key-here

# External Services
REACT_APP_MAP_API_KEY=your-google-maps-api-key
REACT_APP_NOTIFICATION_SERVICE_URL=your-notification-service-url