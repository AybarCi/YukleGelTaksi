{
  "expo": {
    "name": "Yükle Gel Taksi - Müşteri",
    "slug": "yukle-gel-taksi-customer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "center",
      "backgroundColor": "#f8f9fa"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yuklegeltaksi.customer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yuklegeltaksi.customer",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-notifications"
    ],
    "scheme": "yukle-gel-customer",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "API_HOST": "192.168.1.134",
      "API_PORT": 3000,
      "SOCKET_PORT": 3000,
      "NODE_ENV": "development",
      "PROD_SOCKET_URL": "https://socket.yuklegeltaksi.com",
      "PROD_API_URL": "https://api.yuklegeltaksi.com"
    }
  }
}
