{
  "name": "yukle-gel-taksi-monorepo",
  "version": "1.0.0",
  "description": "YukleGel Taksi Monorepo - Customer and Driver Mobile Apps",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:all": "npm run build --workspaces",
    "build:shared": "npm run build --workspace=packages/shared",
    "build:customer": "npm run build --workspace=packages/customer-app",
    "build:driver": "npm run build --workspace=packages/driver-app",
    "start:customer": "npm run start --workspace=packages/customer-app",
    "start:driver": "npm run start --workspace=packages/driver-app",
    "start:backend": "cd backend && npm start",
    "dev:customer": "npm run dev --workspace=packages/customer-app",
    "dev:driver": "npm run dev --workspace=packages/driver-app",
    "test:all": "npm run test --workspaces",
    "lint:all": "npm run lint --workspaces",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/dist",
     "reset": "npm run clean && npm install"
   },
   "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "taxi",
    "react-native",
    "expo",
    "monorepo",
    "typescript"
  ],
  "author": "YukleGel Taksi Team",
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "expo": "54",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.7",
    "expo-constants": "~18.0.8",
    "expo-crypto": "~15.0.7",
    "expo-document-picker": "~14.0.7",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.11",
    "expo-router": "~6.0.1",
    "expo-splash-screen": "~31.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-maps": "^1.20.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "socket.io-client": "^4.8.1",
    "react-native-worklets": "0.5.1"
  }
}
